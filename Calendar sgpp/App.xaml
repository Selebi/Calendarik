<Application x:Class="PlanSelector.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PlanSelector"
             StartupUri="Controls/Plans.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/RadioDate.xaml" />
                <ResourceDictionary Source="Styles/LiteButton.xaml" />
                <ResourceDictionary Source="Styles/LiteButtonDark.xaml" />
                <ResourceDictionary Source="Styles/LiteButtonDarkCalendar.xaml" />
                <ResourceDictionary Source="Styles/Tile.xaml" />
                <ResourceDictionary Source="Styles/List.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

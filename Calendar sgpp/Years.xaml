<UserControl x:Class="Calendar_sgpp.Years"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Calendar_sgpp"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="200">
    <Grid x:Name="YearGrid">
        <Button Content="&gt;" Click="NextYear" HorizontalAlignment="Right" Height="32" Width="32" FontSize="24" FontFamily="NSimSun" FontWeight="Bold" Style="{StaticResource LiteButton}"/>
        <Button Content="&lt;" Click="PreviousYear" HorizontalAlignment="Left" Height="32" Width="32" FontSize="24" FontFamily="NSimSun" FontWeight="Bold" Style="{StaticResource LiteButton}"/>
    </Grid>
</UserControl>
